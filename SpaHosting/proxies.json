{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "api": {
      "matchCondition": {
        "route": "/api/{*url}",
        "methods": ["GET"]
      },
      "backendUri": "https://localhost/api/{url}"
    },
    "api with route parameter": {
      "disabled": true,
      "matchCondition": {
        "route": "/api/users/{userId}/cars",
        "methods": ["GET"]
      },
      "backendUri": "https://localhost/api/users/{userId}/cars"
    }
  }
}
